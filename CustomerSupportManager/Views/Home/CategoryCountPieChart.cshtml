@model IEnumerable<CustomerSupportManager.Models.CategoryCountModel>

@{
    var myChart = new Chart(width: 500, height: 300)
        .AddTitle("Ticket count by category (Pie Chart)")
        .AddSeries(
            name: "categoryCount",
            chartType: "Pie",
            xValue: Model, xField: "Category",
            yValues: Model, yFields: "Count")
        .Write("png");
}